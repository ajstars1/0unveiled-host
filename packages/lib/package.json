  "devDependencies": {
    "@0unveiled/eslint-config": "*",
    "@0unveiled/typescript-config": "*",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^8.57.0",
