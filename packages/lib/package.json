{
  "name": "@0unveiled/lib",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "license": "MIT",
  "exports": {
    "./utils": {
      "types": "./src/utils.ts",
      "import": "./dist/utils.mjs",
      "require": "./dist/utils.js"
    },
    "./supabase": {
      "types": "./src/supabase/index.ts",
      "import": "./dist/supabase/index.mjs",
      "require": "./dist/supabase/index.js"
    },
    "./constants": {
      "types": "./src/constants.ts",
      "import": "./dist/constants.mjs",
      "require": "./dist/constants.js"
    },
    "./validations": {
      "types": "./src/validations/index.ts",
