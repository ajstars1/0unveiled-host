{
  "name": "@0unveiled/ui",
  "version": "0.0.0",
  "sideEffects": false,
  "license": "MIT",
  "exports": {
    "./styles.css": "./dist/index.css",
    "./*": "./dist/*.js",
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./components/button": {
      "types": "./src/components/button.tsx",
      "import": "./dist/components/button.mjs",
      "require": "./dist/components/button.js"
    },
    "./components/card": {
      "types": "./src/components/card.tsx",
      "import": "./dist/components/card.mjs",
      "require": "./dist/components/card.js"
    },
    "./components/input": {
      "types": "./src/components/input.tsx",
      "import": "./dist/components/input.mjs",
      "require": "./dist/components/input.js"
    },
    "./components/label": {
      "types": "./src/components/label.tsx",
      "import": "./dist/components/label.mjs",
      "require": "./dist/components/label.js"
    },
    "./components/dialog": {
      "types": "./src/components/dialog.tsx",
      "import": "./dist/components/dialog.mjs",
      "require": "./dist/components/dialog.js"
    },
    "./components/toast": {
      "types": "./src/components/toast.tsx",
      "import": "./dist/components/toast.mjs",
      "require": "./dist/components/toast.js"
    },
    "./lib/utils": {
      "types": "./src/lib/utils.ts",
      "import": "./dist/lib/utils.mjs",
      "require": "./dist/lib/utils.js"
    },
    "./hooks/use-mobile": {
      "types": "./src/hooks/use-mobile.ts",
      "import": "./dist/hooks/use-mobile.mjs",
      "require": "./dist/hooks/use-mobile.js"
    }
  },
  "scripts": {
    "build": "npm run build:styles && npm run build:components",
    "build:styles": "tailwindcss -i ./src/styles.css -o ./dist/index.css",
    "build:components": "tsup",
    "dev": "npm run dev:styles & npm run dev:components",
    "dev:styles": "tailwindcss -i ./src/styles.css -o ./dist/index.css --watch",
    "dev:components": "tsup --watch",
    "lint": "eslint . --max-warnings 0",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .turbo node_modules dist"
  },
  "devDependencies": {
    "@0unveiled/config": "*",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "eslint": "^8.57.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2",
    "@repo/tailwind-config": "workspace:*",
    "@tailwindcss/cli": "^4.1.11",
    "@tailwindcss/postcss": "^4.1.11",
    "autoprefixer": "^10.4.20",
    "tailwindcss": "^4.1.11"
  },
  "dependencies": {
    "@0unveiled/lib": "*",
    "@hookform/resolvers": "^5.1.1",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-context-menu": "^2.2.15",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-hover-card": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.15",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.0",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.525.0",
    "next-themes": "^0.4.6",
    "react": "^18.3.0",
    "react-day-picker": "^9.8.0",
    "react-hook-form": "^7.60.0",
    "react-resizable-panels": "^3.0.3",
    "recharts": "2.15.4",
    "sonner": "^2.0.6",
    "tailwind-merge": "^2.2.1",
    "vaul": "^1.1.2",
    "zod": "^4.0.5"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
